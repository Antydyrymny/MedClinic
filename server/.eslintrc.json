{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off",
        "no-unused-vars": "warn",
        "no-underscore-dangle": "off"
    }
}
